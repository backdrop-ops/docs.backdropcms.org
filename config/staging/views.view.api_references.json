                    "file_name": {
                        "id": "file_name",
                        "table": "api_documentation",
                        "field": "file_name",
                        "relationship": "from_did",
                        "label": "Location",
                        "link_to_docs": "file_base"
                    },
                    "summary": {
                        "id": "summary",
                        "table": "api_documentation",
                        "field": "summary",
                        "relationship": "from_did",
                        "label": "Description"
                    }
                },
                "arguments": {
                    "branch_id": {
                        "id": "branch_id",
                        "table": "api_reference_storage",
                        "field": "branch_id",
                        "default_action": "not found",
                        "default_argument_type": "fixed",
                        "summary": {
                            "number_of_records": "0",
                            "format": "default_summary"
