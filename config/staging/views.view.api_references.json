                            "default_sort_order": "asc",
                            "align": "",
                            "separator": "",
                            "empty_column": 0
                        },
                        "summary": {
                            "sortable": 0,
                            "default_sort_order": "asc",
                            "align": "",
                            "separator": "",
                            "empty_column": 0
                        }
                    }
                },
                "relationships": {
                    "from_did": {
                        "id": "from_did",
                        "table": "api_reference_storage",
                        "field": "from_did",
                        "label": "Referencing object",
