                        "summary_options": {
                            "items_per_page": "25"
                        }
                    },
                    "object_name": {
                        "id": "object_name",
                        "table": "api_reference_storage",
                        "field": "object_name",
                        "default_action": "not found",
                        "default_argument_type": "fixed",
                        "summary": {
                            "number_of_records": "0",
                            "format": "default_summary"
                        },
                        "summary_options": {
                            "items_per_page": "25"
                        },
                        "limit": "0"
