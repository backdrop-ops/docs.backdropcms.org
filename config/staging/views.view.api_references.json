                        "summary_options": {
                            "items_per_page": "25"
                        },
                        "limit": "0"
                    }
                },
                "filters": {
                    "object_type": {
                        "id": "object_type",
                        "table": "api_reference_storage",
                        "field": "object_type",
                        "value": "group"
                    }
                }
            }
